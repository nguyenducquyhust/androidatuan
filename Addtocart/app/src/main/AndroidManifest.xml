<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.addtocart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AddToCart">
        <activity
            android:name="com.example.nav.NavActivity"
            android:label="@string/title_activity_nav"
            android:theme="@style/Theme.AddToCart.NoActionBar"></activity>
        <activity android:name="com.example.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.navigation.NavigationActivity"
            android:label="@string/title_activity_navigation"
            android:theme="@style/Theme.AddToCart.NoActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>